msgid "Liberty"
msgstr ""

#: Liberty.cfg:17
msgid "(Easy)"
msgstr ""

#: Liberty/units/Thug-peasant.cfg:5 Liberty.cfg:17
msgid "Peasant"
msgstr ""

#: Liberty/units/Outlaw.cfg:5 Liberty.cfg:18
msgid "Outlaw"
msgstr ""

